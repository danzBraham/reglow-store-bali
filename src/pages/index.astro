import Unggul from "../components/Unggul.astro";
import Bahan from "../components/Bahan.astro";
import Testimoni from "../components/Testimoni.tsx";
import Paket from "../components/Paket.astro";
import ProdukPopuler from "../components/ProdukPopuler.astro";
---

<Layout title="Reglow Store Bali">
